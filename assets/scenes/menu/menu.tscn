[sub_resource type="GDScript" id="GDScript_h14ep"]
script/source = "extends Node3D
@onready var menuSong = $menuMusic
@onready var menuSound = $menuSound

# Called when the node enters the scene tree for the first time.
func _ready():
	menuSong.play()


func _on_play_button_pressed():
	menuSound.play()
	await Fade.fade_out(0.3, Color(0,0,0,1),\"GradientVertical\",false,true).finished
	get_tree().change_scene_to_file(\"res://assets/scenes/worlds/debug/debugWorld.tscn\")

func _on_quit_button_pressed():
	menuSound.play()
	globalGameManager.save_settings()
	await Fade.fade_out(0.3, Color(0,0,0,1),\"GradientVertical\",false,true).finished
	get_tree().quit()


func _on_menu_music_finished():
	menuSong.play()

"

