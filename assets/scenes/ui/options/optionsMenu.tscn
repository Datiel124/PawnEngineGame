[sub_resource type="GDScript" id="GDScript_i2vi2"]
resource_name = "optionsNode"
script/source = "extends Control


@onready var save_button : Button = $panelHolder/panel/save
@onready var revert_button : Button = $panelHolder/panel/revert


func _ready() -> void:
	UserConfig.configs_updated.connect(apply_from_settings)


func apply_from_settings() -> void:
	print(\"Applying from saved values...\")
	$panelHolder/panel/optionsPanel/Game/vBoxContainer/Fullscreen.button_pressed = UserConfig.graphics_fullscreen


func _on_save_pressed() -> void:
	#Saves all the settings
	UserConfig.graphics_fullscreen = $panelHolder/panel/optionsPanel/Game/vBoxContainer/Fullscreen.button_pressed
	UserConfig.saveConfigs()


func _on_revert_pressed() -> void:
	#Reverts to saved state
	revert_button.disabled = true
	get_tree().create_timer(1.0).timeout.connect(revert_button.set.bind(&\"disabled\", false))
	apply_from_settings()


func _on_close_button_pressed() -> void:
	_on_revert_pressed()
	hide()
"

